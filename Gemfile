# Gemfile for ColdFusion Killer v2
# Grab Sources from the default RubyGems depot
source 'https://rubygems.org'

# Adds a little color with ease
gem 'colorize'

# The core of MSF made available in gem form:
gem 'librex'

# Readline Support cause its nice
gem 'rb-readline'

# Print cool formatted tables with ease
gem 'text-table'
